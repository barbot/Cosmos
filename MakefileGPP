CC=g++
OPTIONS=-Wno-deprecated

all: Generator 
	
Generator: classes
	$(CC) $(OPTIONS) Obj/ModelGenerator/mainGPP.o Obj/ModelGenerator/Eval/Eval-parser.tab.o  Obj/ModelGenerator/Eval/Eval.o \
 Obj/ModelGenerator/GspnParser/Gspn-parser.tab.o  Obj/ModelGenerator/GspnParser/Gspn-Reader.o \
 Obj/ModelGenerator/LhaParser/Lha-Reader.o Obj/ModelGenerator/LhaParser/Lha-parser.tab.o  \
 Obj/Cosmos/Event.o Obj/Cosmos/EventsQueue.o  \
 Obj/Cosmos/StandardNormalDist.o Obj/Cosmos/ap.o \
 -o bin/CosmosGPP

classes: 
	$(CC) $(OPTIONS) -c SOURCES/ModelGenerator/Eval/Eval-parser.tab.cc -o Obj/ModelGenerator/Eval/Eval-parser.tab.o
	$(CC) $(OPTIONS) -c SOURCES/ModelGenerator/Eval/Eval.cpp -o Obj/ModelGenerator/Eval/Eval.o
	$(CC) $(OPTIONS) -c SOURCES/ModelGenerator/GspnParser/Gspn-parser.tab.cc -o Obj/ModelGenerator/GspnParser/Gspn-parser.tab.o
	$(CC) $(OPTIONS) -c SOURCES/ModelGenerator/GspnParser/Gspn-Reader.cpp -o Obj/ModelGenerator/GspnParser/Gspn-Reader.o	
	$(CC) $(OPTIONS) -c SOURCES/ModelGenerator/LhaParser/Lha-parser.tab.cc -o Obj/ModelGenerator/LhaParser/Lha-parser.tab.o
	$(CC) $(OPTIONS) -c SOURCES/ModelGenerator/LhaParser/Lha-Reader.cpp -o Obj/ModelGenerator/LhaParser/Lha-Reader.o
	$(CC) $(OPTIONS) -c SOURCES/ModelGenerator/mainGPP.cpp -o Obj/ModelGenerator/mainGPP.o	
	$(CC) $(OPTIONS) -c SOURCES/Cosmos/Event.cpp -o Obj/Cosmos/Event.o
	$(CC) $(OPTIONS) -c SOURCES/Cosmos/EventsQueue.cpp -o  Obj/Cosmos/EventsQueue.o
	$(CC) $(OPTIONS) -c SOURCES/Cosmos/tab.cpp -o Obj/Cosmos/tab.o
	$(CC) $(OPTIONS) -c SOURCES/Cosmos/RareEvent.cpp -o Obj/Cosmos/RareEvent.o
	$(CC) $(OPTIONS) -c SOURCES/Cosmos/StandardNormalDist.cpp -o Obj/Cosmos/StandardNormalDist.o
	$(CC) $(OPTIONS) -c SOURCES/Cosmos/ap.cpp -o Obj/Cosmos/ap.o


