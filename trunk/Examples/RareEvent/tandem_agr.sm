ctmc
const int n = 5;
const int r = 5;

const double lambda = 0.100000;
const double rho1 = 0.10000;
const double rho2 = 0.10000;

module Tandem
	Queue1 : [0..n] init 1;	Queue2 : [0..r] init 0;
	[] Queue1<n -> lambda : (Queue1'=Queue1+1);
	[] Queue1>0 & Queue2<r -> rho1 : (Queue1'=Queue1-1) & (Queue2'=Queue2+1) ;
	[] Queue2>0 -> rho2 : (Queue2'=Queue2-1) ;
endmodule
formula win = Queue1 + Queue2=n;
formula loose = Queue1 + Queue2=0;